.status_bar {
  height: 25px;
  width: 100%;
}
.login {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 100;
  background: white;
  box-sizing: border-box;
  background-color: #50d8be;
  left: 0;
}
.login .head {
  width: calc(100% - 95px);
  height: 80rpx;
  background-color: #50d8be;
}
.login .head image {
  height: 45rpx;
  width: 45rpx;
  margin-top: 15rpx;
}
.login .head text {
  margin: 15rpx 0 0 290rpx;
  font-size: 35rpx;
  font-weight: bold;
  color: white;
}
.logo {
  margin-top: 150rpx;
  width: 100%;
  height: 200rpx;
}
.logo image {
  width: 200rpx;
  height: 200rpx;
}
.famousremarks {
  display: flex;
  width: 60%;
  height: 60rpx;
  justify-content: space-around;
  align-items: center;
}
.famousremarks text {
  font-style: oblique;
  font-weight: bold;
}
.inputsr {
  margin-top: 30rpx;
  height: 300rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.inputsr .inputone {
  height: 100rpx;
  width: 70%;
  border: 1px solid #50d8be;
}
.inputsr .inputone input {
  height: 100%;
  width: 100%;
}
.inputsr .inputtwo {
  height: 100rpx;
  width: 70%;
  border: 1px solid #50d8be;
}
.inputsr .inputtwo input {
  height: 100%;
  width: 100%;
}
.consult {
  display: flex;
  padding-left: 115rpx;
  align-items: center;
  margin: 40rpx 0;
}
.consult .checkbox {
  background-color: #f0f0f0;
  width: 45rpx;
  height: 45rpx;
  border-radius: 50%;
  margin-right: 25rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 60%;
}
.consult text {
  font-size: 22rpx;
}
.protocol {
  width: 94%;
  box-sizing: border-box;
  margin: 0 3%;
  height: 105rpx;
}
.protocol .btn {
  width: 90%;
  height: 105rpx;
  border-radius: 52rpx;
}
.txta {
  height: 200rpx;
  width: 100%;
  display: flex;
  justify-content: space-around;
}
.txta text {
  font-size: 30rpx;
}

