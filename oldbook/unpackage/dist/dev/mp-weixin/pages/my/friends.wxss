.gd {
  height: 20rpx;
  width: 100%;
  background-color: #000000;
  box-sizing: border-box;
  line-height: 20rpx;
}
.gd .name {
  color: #50d8be;
  font-size: 30rpx;
  justify-content: space-evenly;
  padding-top: 35rpx;
  text-align: center;
}
.gd ._l {
  display: flex;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
}
.gd ._r {
  display: flex;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50rpx;
}
.list-cell {
  display: flex;
  box-sizing: border-box;
  width: 100%;
  padding: 10px 24rpx;
  overflow: hidden;
  color: #323233;
  font-size: 14px;
  line-height: 24px;
  background-color: #fff;
}
.status_bar {
  height: 25px;
  width: 100%;
}
.tou {
  position: fixed;
  height: 100rpx;
  width: 100%;
  z-index: 5;
  top: 0;
  left: 0;
  background-color: #50d8be;
  box-sizing: border-box;
}
.tou .top {
  width: calc(100% - 95px);
  width: 100%;
  height: 70rpx;
  background-color: #50d8be;
  display: flex;
  align-items: center;
}
.tou .top .jt {
  width: 45rpx;
  height: 45rpx;
  margin-left: 50rpx;
  font-size: 45rpx;
}
.tou .top .zi {
  color: #393939;
  font-size: 45rpx;
  border-bottom: 6rpx #FFFFFF solid;
  font-weight: bold;
  margin-left: 190rpx;
}

