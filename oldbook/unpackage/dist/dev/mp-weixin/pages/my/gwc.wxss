.status_bar {
  height: 25px;
  width: 100%;
}
.tou {
  position: fixed;
  height: 100rpx;
  width: 100%;
  z-index: 5;
  top: 0;
  left: 0;
  background-color: #50d8be;
  box-sizing: border-box;
}
.tou .top {
  width: 100%;
  height: 90rpx;
  background-color: #50d8be;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}
.tou .top .jt {
  position: absolute;
  width: 45rpx;
  height: 45rpx;
  left: 50rpx;
  font-size: 45rpx;
}
.tou .top .zi {
  position: absolute;
  color: #393939;
  font-size: 45rpx;
  border-bottom: 6rpx #FFFFFF solid;
  font-weight: bold;
}
.maincontent {
  width: 100%;
}
.maincontent .__bdi {
  margin-top: 200rpx;
  display: flex;
  flex-wrap: wrap;
}
.maincontent .__bdi .tj-item {
  width: 40%;
  padding: 10rpx;
  margin-left: 50rpx;
  margin-bottom: 30rpx;
  height: 500rpx;
  background: linear-gradient(#ffffff, #aaf7ff, #FFFFFF);
  border: 6rpx #42c7e4 solid;
}
.maincontent .__bdi .tj-item .tu {
  width: 100%;
  height: 300rpx;
}
.maincontent .__bdi .tj-item ._s {
  display: flex;
  margin: 10rpx;
  font-weight: bold;
}
.maincontent .__bdi .tj-item ._p {
  display: flex;
  color: #bf0303;
  margin: 10rpx;
}
.maincontent .__bdi .tj-item .shoucang {
  width: 40rpx;
  height: 40rpx;
  margin-left: 60%;
  margin-top: 10rpx;
}
.maincontent .__bdi .tj-item .xuanze {
  width: 40rpx;
  height: 40rpx;
  margin-left: 10rpx;
  margin-top: 10rpx;
}
.maincontent .__bdi .tj-item image {
  width: 40rpx;
  height: 40rpx;
}
.maincontent .empty {
  width: 100%;
  height: 100rpx;
}
.botm {
  position: fixed;
  width: 100%;
  height: 100rpx;
  background-color: #50d8be;
  border-radius: 30rpx 30rpx 0 0;
  bottom: 0;
  display: flex;
}
.botm .allprice {
  width: 230rpx;
  margin-left: 20rpx;
  height: 80rpx;
  font-size: 40rpx;
  background: linear-gradient(#ffffff, #fffaab, #FFFFFF);
  border: 6rpx #d2e42b solid;
  border-radius: 40rpx;
  margin-left: 230rpx;
  line-height: 70rpx;
  text-align: center;
  margin-top: 10rpx;
}
.botm .submit {
  width: 230rpx;
  height: 80rpx;
  font-size: 40rpx;
  margin-left: 20rpx;
  background: linear-gradient(#ffffff, #aaf7ff, #FFFFFF);
  border: 6rpx #2B85E4 solid;
  border-radius: 40rpx;
  line-height: 70rpx;
  text-align: center;
  margin-top: 10rpx;
}

