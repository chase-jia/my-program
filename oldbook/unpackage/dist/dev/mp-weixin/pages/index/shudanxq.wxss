.status_bar {
  height: 25px;
  width: 100%;
}
.head_bg {
  z-index: 100;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  background-color: #50d8be;
  border-radius: 0 0 100rpx 100rpx;
}
.head_bg .head {
  height: 100rpx;
  width: calc(100% - 95px);
}
.head_bg .head image {
  height: 38rpx;
  width: 38rpx;
  margin-left: 50rpx;
  margin-top: 31rpx;
}
.head_bg .head .head_txt {
  font-size: 45rpx;
  font-weight: bold;
  color: white;
  margin-left: 215rpx;
}
