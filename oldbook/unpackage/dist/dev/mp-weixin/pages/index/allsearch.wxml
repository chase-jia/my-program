<view><view class="head_bg"><view class="status_bar"></view><view class="head"><image src="../../static/shudan/1.png" mode data-event-opts="{{[['tap',[['toBack',['$event']]]]]}}" bindtap="__e"></image><text class="head_txt">全部分类</text></view></view><view class="page"><view class="left"><scroll-view style="height:100%;" scroll-y="true"><block wx:for="{{menu}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeChoose',[index]]]]]}}" class="{{['item',(index==choose)?'active':'']}}" bindtap="__e">{{item}}</view></block></scroll-view></view><view class="right"><scroll-view style="height:100%;" scroll-y="true" scroll-into-view="{{whichId}}" scroll-with-animation="{{true}}"><block wx:for="{{goods_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><text class="title" id="{{item.id}}">{{item.name}}</text><view><block wx:for="{{item.list}}" wx:for-item="goodsItem" wx:for-index="goodsIndex" wx:key="goodsIndex"><view data-event-opts="{{[['tap',[['toDetail',['$0'],[[['goods_list','',index],['list','',goodsIndex,'id']]]]]]]}}" class="good_item" bindtap="__e"><image class="good_img" src="{{goodsItem.pic}}"></image><view><view class="name">{{''+goodsItem.name+''}}</view><view class="price">{{''+goodsItem.zuozhe+''}}</view></view></view></block></view></view></block></scroll-view></view></view></view>